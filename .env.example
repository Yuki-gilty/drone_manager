# Supabase Configuration
# Copy this file to .env and fill in your Supabase project credentials
# Get these values from: https://app.supabase.com/project/[PROJECT_ID]/settings/api

VITE_SUPABASE_URL=https://[PROJECT_REF].supabase.co
VITE_SUPABASE_ANON_KEY=[YOUR_ANON_KEY]

# Optional: For direct database connection (not needed for REST API)
# DATABASE_URL=postgresql://postgres:[PASSWORD]@db.[PROJECT_REF].supabase.co:5432/postgres

